class Solution {
    public int subarraySum(int[] nums) {
        int n=nums.length;
        int ans=0;
        int[] p=new int[n+1];
        for(int i=0;i<n;i++){
            p[i+1]=p[i]+nums[i];
        }
        for(int i=0;i<n;i++){
            int start=Math.max(0,i-nums[i]);
            ans+=p[i+1]-p[start];
        }
        return ans;
    }
}
